<?xml version="1.0" encoding="utf-8"?>



<MotionScene xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:motion="http://schemas.android.com/apk/res-auto">
    <Transition
        motion:constraintSetEnd="@+id/end"
        motion:constraintSetStart="@id/start"
        motion:duration="1000">
        <KeyFrameSet>
        </KeyFrameSet>
    </Transition>

    <ConstraintSet android:id="@+id/start">
        <Constraint
            android:id="@+id/startButton"
            motion:layout_constraintEnd_toStartOf="@+id/guideline3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            motion:layout_constraintBottom_toTopOf="@+id/palette_and_brush_imageview"
            motion:layout_constraintTop_toBottomOf="@+id/splash_imageview"
            motion:layout_constraintStart_toStartOf="@+id/guideline2"
            android:visibility="invisible" >
            <CustomAttribute
                motion:attributeName="enabled"
                motion:customBoolean="false" />
        </Constraint>
        <Constraint
            android:id="@+id/startButton"
            motion:layout_constraintEnd_toStartOf="@+id/guideline3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            motion:layout_constraintBottom_toTopOf="@+id/palette_and_brush_imageview"
            motion:layout_constraintTop_toBottomOf="@+id/splash_imageview"
            motion:layout_constraintStart_toStartOf="@+id/guideline2"
            android:visibility="visible" />
        <Constraint
            android:id="@+id/startButton"
            motion:layout_constraintEnd_toStartOf="@+id/guideline3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            motion:layout_constraintBottom_toTopOf="@+id/palette_and_brush_imageview"
            motion:layout_constraintTop_toBottomOf="@+id/splash_imageview"
            motion:layout_constraintStart_toStartOf="@+id/guideline2"
            android:visibility="invisible" />
    </ConstraintSet>

    <ConstraintSet android:id="@+id/end">
        <Constraint
            android:id="@+id/startButton"
            motion:layout_constraintEnd_toStartOf="@+id/guideline3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            motion:layout_constraintBottom_toTopOf="@+id/palette_and_brush_imageview"
            motion:layout_constraintTop_toBottomOf="@+id/splash_imageview"
            motion:layout_constraintStart_toStartOf="@+id/guideline2"
            android:visibility="visible" >
            <CustomAttribute
                motion:attributeName="enabled"
                motion:customBoolean="true" />
        </Constraint>
    </ConstraintSet>
</MotionScene>
